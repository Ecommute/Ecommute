<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#00FFFFFF"
    android:backgroundTint="#FF9800"
    android:focusableInTouchMode="true"
    android:clickable="true"
    android:focusable="true"
    android:foregroundTint="#00EDEDED">

    <TextView
        android:id="@+id/puntos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="34dp"
        android:text="TextView"
        android:textColor="#4CAF50"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/NombreEvento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:translationZ="10dp"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/item_button" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="@string/pre_origen"
        android:textSize="16sp"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintStart_toEndOf="@+id/originIcon2"
        app:layout_constraintTop_toTopOf="@+id/item_button" />

    <TextView
        android:id="@+id/HoraEvento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="TextView"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/item_button"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/dconsumo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="12dp"
        android:text="@string/pre_destino"
        android:textStyle="bold"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/item_button"
        app:layout_constraintStart_toEndOf="@+id/destIcon2" />

    <Button
        android:id="@+id/item_button"
        android:layout_width="379dp"
        android:layout_height="93dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#EBFFEB"
        android:clickable="true"
        android:translationZ="-10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/starButton"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#00FFFFFF"
        android:translationZ="100dp"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/item_button"
        app:layout_constraintEnd_toEndOf="@+id/item_button"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/destIcon2"
        android:layout_width="28dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="9dp"
        android:translationZ="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/item_button"
        app:layout_constraintStart_toStartOf="@+id/item_button"
        app:srcCompat="@drawable/ic_baseline_place_24" />

    <ImageView
        android:id="@+id/originIcon2"
        android:layout_width="28dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="7dp"
        android:translationZ="5dp"
        app:layout_constraintStart_toStartOf="@+id/item_button"
        app:layout_constraintTop_toTopOf="@+id/item_button"
        app:srcCompat="@drawable/start" />


</androidx.constraintlayout.widget.ConstraintLayout>